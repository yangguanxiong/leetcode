/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public ListNode rotateRight(ListNode head, int n) {
        if (head == null || n == 0) return head;
        ListNode r1 = head;
        int l = 1;
        while(r1.next != null){
            r1 = r1.next;
            l++;
        }
        n %= l;
        r1.next = head;
        ListNode prev = head;
        ListNode newhead = head;
        while(l - n > 1){
            prev = prev.next;
            newhead = newhead.next;
            l--;
        }
        newhead = newhead.next;
        prev.next = null;
        return newhead;
    }
}
