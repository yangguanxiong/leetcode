 public ListNode deleteDuplicates(ListNode head) {
        // Start typing your Java solution below
        // DO NOT write main() function
        if (head == null || head.next == null) return head;
        
        ListNode pre = head;
        ListNode cur = head;
        
        cur = cur.next;
        
        while(cur != null ) {
            if (pre.val != cur.val){
                pre = pre.next;
                cur = cur.next;
               // continue;
            } else {
                while( cur!= null && cur.val == pre.val ){
                    pre.next = cur.next;
                    cur = cur.next;
                }
            }
            
        }
        return head;
    }
